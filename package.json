  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/will-wow/pipeout.git"
  },
  "engines": {
    "node": ">=8.11.3"
  },
  "keywords": [
    "pipe",
    "functional",
    "async",
    "promise",
    "typescript",
    "javascript"
  ],
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "babel-cli": "^6.26.0",
    "codecov": "^3.6.1",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "rollup": "^1.23.1",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.4"
  },
  "scripts": {
    "build": "rollup -c",
    "docs": "typedoc && cp -R assets/* docs/assets/ && cp favicon.ico docs",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "test": "jest",
    "format": "npm-run-all -c format:*",
    "format:eslint": "eslint --fix '**/*.ts'",
    "format:prettier": "prettier --write '**/*.{ts,json,md}'",
    "format:types": "tsc --noEmit",
    "lint": "npm-run-all -c lint:*",
    "lint:eslint": "eslint '**/*.ts'",
    "lint:prettier": "prettier --check '**/*.{ts,json,md}'",
    "lint:types": "tsc --noEmit"
  }
}
